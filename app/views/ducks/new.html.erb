<%= form_for @duck, url: {action: "create"}, html: {class: "whatever"} do |f|%>
<%= f.text_field :name %>
<%= f.label :name %>
<%= f.text_field :description %>
<%= f.label :description %>
<%= f.number_field :age %>
<%= f.label :age %>
<%= f.number_field :gender %>
<%= f.label :gender %>
<%= f.submit "Make a Duck" %>
<% end %>